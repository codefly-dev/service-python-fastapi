kind: service
name: {{.Service.Name.Title}}
application: {{.Service.Name.Title}}
namespace: default
version: 0.0.0
plugin:
    kind: runtime::service
    name: python-fastapi
    version: latest
    publisher: codefly.ai
endpoints:
    - name: grpc
      api: grpc
    - name: rest
      public: true
      api: rest
spec:
    create-rest-endpoint: true
    debug: false
    watch: true
    with-debug-symbols: false
