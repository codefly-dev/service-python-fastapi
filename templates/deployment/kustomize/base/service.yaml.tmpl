apiVersion: v1
kind: Service
metadata:
  name: {{ .Service.Unique.DNSCase }}
  namespace: {{ .Service.Namespace }}
spec:
  selector:
    app: {{ .Service.Unique.DNSCase}}
  ports:
    - protocol: TCP
      name: http-port
      port: 8080
      targetPort: 8080
    - protocol: TCP
      name: grpc-port
      port: 9090
      targetPort: 9090
